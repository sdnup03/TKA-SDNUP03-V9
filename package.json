{
  "name": "ipa-exam-room",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:fresh": "npm run clean:all && npm run dev",
    "build": "vite build",
    "preview": "vite preview",
    "clean": "rm -rf node_modules/.vite .vite dist",
    "clean:all": "lsof -ti:3000 | xargs kill -9 2>/dev/null || true && rm -rf node_modules/.vite .vite dist && find . -name '*.cache' -type d -not -path './node_modules/*' -exec rm -rf {} + 2>/dev/null || true",
    "clean:dev": "npm run clean && npm run dev",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2",
    "katex": "^0.16.27",
    "lucide-react": "^0.555.0",
    "mathlive": "^0.108.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jspdf": "^1.3.3",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/ui": "^2.1.8",
    "jsdom": "^25.0.1",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^2.1.8"
  }
}
